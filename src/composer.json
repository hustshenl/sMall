{
  "name": "sMall",
  "description": "基于Yii2的商城系统",
  "keywords": [
    "yii2",
    "mall",
    "o2o",
    "b2c",
    "b2b"
  ],
  "homepage": "http://small.shenl.com/",
  "type": "project",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/hustshenl/sMall/issues?state=open",
    "forum": "http://small.shenl.com/",
    "wiki": "https://github.com/hustshenl/sMall/wiki/",
    "source": "https://github.com/hustshenl/sMall"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.5.9",
    "yiisoft/yii2": "*",
    "yiisoft/yii2-bootstrap": "*",
    "yiisoft/yii2-swiftmailer": "*",
    "yiisoft/yii2-jui": "~2.0",
    "yiisoft/yii2-imagine": "~2.0",
    "yiisoft/yii2-redis": "~2.0.0",
    "yiisoft/yii2-sphinx": "~2.0.0",
    "yiisoft/yii2-httpclient": "~2.0.0",
    "creocoder/yii2-flysystem": "*",
    "dmstr/yii2-adminlte-asset": "2.*",
    "mdmsoft/yii2-admin": "~2.0",
    "hustshenl/yii2-ucenter": "dev-master",
    "hustshenl/yii2-pinyin": "dev-master",
    "hustshenl/yii2-cropper": "dev-master",
    "overtrue/wechat": "3.*",
    "kartik-v/yii2-icons": "*",
    "kartik-v/yii2-widgets": "*",
    "kartik-v/yii2-editable": "*",
    "kartik-v/yii2-detail-view": "*",
    "kartik-v/yii2-dynagrid": "*",
    "kartik-v/yii2-tree-manager": "*",
    "kartik-v/yii2-date-range": "*",
    "kartik-v/yii2-money": "*",
    "kartik-v/yii2-slider": "*",
    "kartik-v/yii2-checkbox-x": "*",
    "kartik-v/yii2-sortable-input": "*",
    "kartik-v/yii2-builder": "*",
    "piwik/device-detector": "*",
    "xj/yii2-oauth": "*"
  },
  "require-dev": {
    "yiisoft/yii2-codeception": "*",
    "yiisoft/yii2-debug": "*",
    "yiisoft/yii2-gii": "*",
    "yiisoft/yii2-faker": "*"
  },
  "config": {
    "process-timeout": 1800
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower",
      "npm-searchable": false,
      "bower-searchable": false
    }
  }
}
