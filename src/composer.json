{
  "name": "hustshenl/s-mall",
  "description": "基于Yii2的商城系统",
  "keywords": [
    "yii2",
    "mall",
    "o2o",
    "b2c",
    "b2b"
  ],
  "homepage": "http://small.shenl.com/",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Shen Lei",
      "email": "shen@shenl.com",
      "homepage": "http://www.shenl.com/",
      "role": "Founder and project lead"
    }
  ],
  "support": {
    "issues": "https://github.com/hustshenl/sMall/issues?state=open",
    "forum": "http://small.shenl.com/",
    "wiki": "https://github.com/hustshenl/sMall/wiki/",
    "source": "https://github.com/hustshenl/sMall"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.5.9",
    "yiisoft/yii2": "~2.0",
    "yiisoft/yii2-bootstrap": "~2.0",
    "yiisoft/yii2-swiftmailer": "~2.0",
    "yiisoft/yii2-jui": "~2.0",
    "yiisoft/yii2-imagine": "~2.0",
    "yiisoft/yii2-redis": "~2.0",
    "yiisoft/yii2-sphinx": "~2.0",
    "yiisoft/yii2-httpclient": "~2.0",
    "creocoder/yii2-flysystem": "~0.8",
    "dmstr/yii2-adminlte-asset": "~2.3",
    "mdmsoft/yii2-admin": "~2.0",
    "hustshenl/yii2-ucenter": "~0.1",
    "hustshenl/yii2-pinyin": "~0.1",
    "hustshenl/yii2-cropper": "~0.1",
    "overtrue/wechat": "~3.1",
    "urbanindo/yii2-queue": "~1.3",
    "kartik-v/yii2-icons": "~1.4",
    "kartik-v/yii2-widgets": "~3.4",
    "kartik-v/yii2-editable": "~1.7",
    "kartik-v/yii2-detail-view": "~1.7",
    "kartik-v/yii2-dynagrid": "~1.4",
    "kartik-v/yii2-tree-manager": "~1.0",
    "kartik-v/yii2-date-range": "~1.6",
    "kartik-v/yii2-money": "~1.2",
    "kartik-v/yii2-slider": "~1.3",
    "kartik-v/yii2-checkbox-x": "~1.0",
    "kartik-v/yii2-sortable-input": "~1.1",
    "kartik-v/yii2-builder": "~1.6",
    "piwik/device-detector": "~3.7",
    "xj/yii2-oauth": "~2.0",
    "bower-asset/toastr":"~2.1.1"
  },
  "require-dev": {
    "yiisoft/yii2-codeception": "~2.0",
    "yiisoft/yii2-debug": "~2.0",
    "yiisoft/yii2-gii": "~2.0",
    "yiisoft/yii2-faker": "~2.0"
  },
  "config": {
    "process-timeout": 1800
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower",
      "npm-searchable": false,
      "bower-searchable": false
    }
  }
}
